{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console" : "off",
        "linebreak-style" : "off",
        "object-property-newline" : "off",
        "indent" : "off",
        "no-unused-vars": "off"
    }
}
